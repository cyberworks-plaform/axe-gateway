{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
    "MinimumLevel": {
      "Default": "Information"
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "logs\\log.gateway..txt",
          "fileSizeLimitBytes": 100000,
          "retainedFileCountLimit": 10,
          "rollingInterval": "Day",
          "rollOnFileSizeLimit": true
        }
      }
    ]
  },
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "ServiceCode": "Gateway",
  "Tokens": {
    "Key": "0123456789ABCDEF",
    "Issuer": "http://ce.com.vn",
    "Audience": "http://ce-axe.com.vn"
  },
  "HealthChecksUI": {
    "EvaluationTimeInSeconds": 60,
    "MinimumSecondsBetweenFailureNotifications": 300,
    "HealthChecks": [
      {
        "Name": "Fake OCR node 1",
        "Uri": "http://localhost:10501/health"
      },
      {
        "Name": "Fake OCR node 2",
        "Uri": "http://localhost:10502/health"
      }
    ]

  },
  "RequestReport": {
    "AggregationIntervalMinutes": 5,
    "AggregationLookbackDays": 90,
    "CacheDefaultTtlMinutes": 30,
    "CacheShortTtlMinutes": 2
  }

}
